class Solution {
public:
    bool arrayStringsAreEqual(vector<string>& word1, vector<string>& word2) {
        string w1,w2;
        for(string word : word1){
         w1=w1 + word;}
     for(string wordy : word2){
        w2=w2 + wordy;    
    }
    if(w1==w2)
{
    return true;}
    else
    return false;
}};
